<h1><%= @comedian.name %></h1>
<section class="card">
  <p class="card-text">Age: <%= @comedian.age %> |
  City: <%= @comedian.city %> </p>
  <% @comedian.specials.each do |sp| %>
    <p>Name: <%= sp.name %> | 
    Length: <%= sp.length %></p>
    <img class="special-img" src="<%= sp.image %>" alt=" <%= sp.name %> ">
  <% end %>
</section>
